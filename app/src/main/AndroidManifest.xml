<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nitj.utkansh.utkansh_app">

    <!-- Permissions -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <!-- Permission: Allow application to write to SDCard -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <permission
        android:name="com.nitj.utkansh.utkansh_app.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="com.nitj.utkansh.utkansh2016.permission.C2D_MESSAGE" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />

    <application
        android:name=".app.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Utkansh"
            android:label="@string/app_name"
            android:noHistory="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Home"
            android:label="@string/title_activity_home"
            android:screenOrientation="portrait" />
        <activity
            android:name=".WebViewActivity"
            android:label="Web View"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MyProfile"
            android:label="My Profile"
            android:parentActivityName=".Home"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.nitj.utkansh.utkansh_app.Home" />
        </activity>
        <activity
            android:name=".Bookmarks"
            android:label="@string/title_activity_bookmarks"
            android:parentActivityName=".Home"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.nitj.utkansh.utkansh_app.Home" />
        </activity>
        <activity
            android:name=".Maps"
            android:label="@string/title_activity_maps"
            android:parentActivityName=".Home"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.nitj.utkansh.utkansh_app.Home" />
        </activity>
        <activity
            android:name=".Notifications"
            android:label="@string/title_activity_notifications"
            android:parentActivityName=".Home"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.nitj.utkansh.utkansh_app.Home" />
        </activity>
        <!-- Google MAP API key -->

        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyDYfqyC0b-eYSdcJR5s1nchDncU0YWZM1A" />

        <activity
            android:name=".GpsCheck"
            android:label="@string/title_activity_gps_check"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MapsTest"
            android:label="@string/title_activity_maps_test"
            android:screenOrientation="portrait" />
        <activity
            android:name=".EventList"
            android:label="List of Events"
            android:screenOrientation="portrait"
            android:theme="@style/MyEventTheme"
            />
        <activity
            android:name=".Event"
            android:label="Event Description"
            android:screenOrientation="portrait"
            />
      <!--  android:theme="@style/AppTheme.NoActionBar"-->

        <activity
            android:name=".Merchandise"
            android:label="@string/title_activity_merchandise"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MapsTestEvents"
            android:label="@string/title_activity_maps_test_events"
            android:screenOrientation="portrait" />
        <activity
            android:name=".GpsCheckEvents"
            android:label="@string/title_activity_gps_check_events"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LoginReg"
            android:label="@string/title_activity_login_reg"
            android:noHistory="true"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.LoginActivity"
            android:label="@string/app_name"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".activity.RegisterActivity"
            android:label="@string/app_name"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />

        <receiver
            android:name=".activity.GcmBroadcastReceiver"
            android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <action android:name="com.google.android.c2dm.intent.REGISTRATION" />

                <category android:name="com.nitj.utkansh.utkansh2016" />
            </intent-filter>
        </receiver>

        <service android:name=".activity.GCMNotificationIntentService" />

        <activity
            android:name=".SearchActivity"
            android:label="@string/title_activity_search_activity"
            android:noHistory="true"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Developers"
            android:label="@string/title_activity_developers"
            android:parentActivityName=".Home"
            android:screenOrientation="portrait"></activity>

        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>

</manifest>